function [dist] = HSV_dist(X1, X2)

disp('::')
whos
dh = min(    abs(360.*(X1(1)-X2(:,1)) ),(360- 360.*( abs(X1(1)-X2(:,1) )   ))) ./ 180;

disp(['dh: ',num2str(max(dh)),'  ', num2str(min(dh))])
ds = abs(X1(:,2)-X2(:,2));
% disp(['ds: ',num2str(max(ds)),'  ', num2str(min(ds))])
dv = abs(X1(:,3)-X2(:,3));
% disp(['dv: ',num2str(max(dv)),'  ', num2str(min(dv))])

dist = sqrt(dh.^2+ds.^2+dv.^2);
% disp(['dist: ',num2str(max(dist)),'  ', num2str(min(dist))])
end